<cal:calendar-query xmlns:cal="urn:ietf:params:xml:ns:caldav"
                  xmlns:cs="http://calendarserver.org/ns/"
                  xmlns:d="DAV:">
  <d:prop>
    {{#each props}}{{prop this}}{{/each}}
  </d:prop>
  <cal:filter>
    {{#each filters}}{{filter this}}{{/each}}
  </cal:filter>
  {{#if timezone}}<cal:timezone>{{timezone}}</cal:timezone>{{/if}}
</cal:calendar-query>
